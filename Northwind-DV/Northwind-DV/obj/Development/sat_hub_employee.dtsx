	                  + ISNULL([Address],'') + ISNULL([City],'')  + ISNULL([Region],'') + ISNULL([PostalCode],'') 
					  + ISNULL([Country],'') + ISNULL([HomePhone],'') +  ISNULL([Extension],'') 
